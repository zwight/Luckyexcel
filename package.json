{
  "name": "@zwight/luckyexcel",
  "version": "1.1.3",
  "description": "An excel import and export library adapted to Luckysheet",
  "main": "dist/luckyexcel.cjs.js",
  "module": "dist/luckyexcel.esm.js",
  "browser": "dist/luckyexcel.umd.js",
  "typings": "dist/main.esm.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/main.esm.d.ts",
        "default": "./dist/luckyexcel.esm.js"
      },
      "require": {
        "types": "./dist/main.umd.d.ts",
        "default": "./dist/luckyexcel.cjs.js"
      }
    }
  },
  "scripts": {
    "build": "gulp build",
    "dev": "gulp dev",
    "release": "release-it"
  },
  "keywords": [
    "Luckysheet",
    "excel"
  ],
  "author": "zwight0705@gmail.com",
  "homepage": "https://github.com/zwight/Luckyexcel",
  "repository": {
    "type": "git",
    "url": "https://github.com/zwight/Luckyexcel.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@release-it/conventional-changelog": "^10.0.1",
    "@rollup/plugin-typescript": "^6.1.0",
    "@types/node": "^22.10.2",
    "@types/papaparse": "^5.3.15",
    "@types/xml2js": "^0.4.14",
    "babelify": "^10.0.0",
    "browser-sync": "^3.0.3",
    "browserify": "^17.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "delete": "^1.1.0",
    "fancy-log": "^1.3.3",
    "gulp": "^5.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-uglify": "^3.0.2",
    "gulp-util": "^3.0.8",
    "husk": "^0.5.3",
    "release-it": "^19.0.1",
    "rollup": "^2.33.1",
    "rollup-plugin-typescript2": "^0.29.0",
    "tsify": "^5.0.4",
    "tslib": "^2.0.3",
    "typescript": "~5.3.0",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.11.1"
  },
  "dependencies": {
    "@progress/jszip-esm": "^1.0.3",
    "@univerjs/core": "^0.6.0",
    "@zwight/exceljs": "4.4.2",
    "dayjs": "^1.10.6",
    "nanoid": "^3.3.7",
    "papaparse": "^5.5.2",
    "xml2js": "^0.6.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
